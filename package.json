{
  "name": "workspace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "workspace:build:watch": "npm run build:watch --workspaces --if-present",
    "dev": "concurrently \"npm run workspace:build:watch\" \"next dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "mock": "json-server --watch mock/db.json --routes mock/routes.json -p 8080"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.28.0",
    "@tanstack/react-query-devtools": "^4.32.6",
    "@types/node": "20.1.2",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.12",
    "next": "13.4.12",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.1.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "json-server": "^0.17.3"
  },
  "workspaces": [
    "packages/*"
  ]
}
